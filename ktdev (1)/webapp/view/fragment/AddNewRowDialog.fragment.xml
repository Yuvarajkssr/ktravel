<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<Dialog title="Add New Expense">
		<l:Grid defaultSpan="L12 M12 S12" width="auto" id="idGrid">
			<l:content>
				<f:SimpleForm id="SimpleFormDisplay354" minWidth="1024" maxContainerCols="2" editable="false" layout="ResponsiveGridLayout" labelSpanL="3"
					labelSpanM="3" emptySpanL="4" emptySpanM="4" columnsL="2" columnsM="2">
					<f:content>
						<Label text="Request ID"/>
						<Input id="id"/>
						<Label text="Expense Type"/>
						<Input id="expenseType"/>
						<Label text="Date"/>
						<DateRangeSelection id="DRSexpense" showFooter="true"/>
						<Label text="Cost Center Code"/>
						<Input id="CCC"/>
						<Label text="Cost Center Name"/>
						<Input id="CCN"/>
						<Label text="Amount"/>
						<Input id="amount"/>
					</f:content>
				</f:SimpleForm>
			</l:content>
		</l:Grid>
		<buttons>
			<Button text="CLOSE" press="onCancelPress"/>
			<Button text="SAVE" press="onSubmitPress"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>